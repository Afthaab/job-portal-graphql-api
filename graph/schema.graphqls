# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User{
  id: ID!
  username: String!
  email: String!
  created_at: String!
  updated_at: String!
}

input NewUser{
  username: String!
  email: String!
  password: String!
}

type Mutation{
  createSignup(input:NewUser!): User!
}
